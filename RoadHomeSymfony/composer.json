{
    "name": "root/slim-road-home",
    "description": "This is a silex php RoadHome API",
    "license": "MIT",
    "authors": [
        {
            "name": "compynerds",
            "email": "compynerds@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "RoadHome\\": "src/"
        }
    },
    "config": {
        "bin-dir": "vendor/bin",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "secure-http": false
    },
    "require": {
        "pimple/pimple": "^3.0",
        "phpmd/phpmd": "^1.5",
        "silex/silex": "~2.0"
    },
    "require-dev": {
        "apigen/apigen": "^4.0",
        "phpunit/phpunit": "*",
        "fzaninotto/faker": "^1.5",
        "peridot-php/peridot": "^1.0",
        "peridot-php/peridot-code-coverage-reporters": "^1.0",
        "peridot-php/peridot-concurrency": "^1.1",
        "peridot-php/peridot-dot-reporter": "^1.0",
        "peridot-php/peridot-prophecy-plugin": "^1.1",
        "peridot-php/peridot-watcher-plugin": "^1.3",
        "peridot-php/leo": "^1.0",
        "phploc/phploc": "^2.0",
        "phpmd/phpmd": "^1.5",
        "sebastian/phpcpd": "^1.4",
        "squizlabs/php_codesniffer": "^2.3"
    }
}
